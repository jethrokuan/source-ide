    "sharedb-string-binding": "^1.0.0",
    "websocket-json-stream": "^0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "browserify": "^13.1.1",
